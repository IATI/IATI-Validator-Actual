<?xml version="1.0" encoding="UTF-8"?>
<iati-activities xmlns:me="http://iati.me" me:testclass="identifiers" version="2.02">
  <me:ignore>1.2.8</me:ignore>
  <me:ignore>1.2.11</me:ignore>

  <!-- 1.1.1 - Starts with reporting org -->
  <iati-activity>
    <iati-identifier>XYZ-123</iati-identifier>
    <reporting-org ref="XYZ"/>
  </iati-activity>

  <iati-activity>
    <me:expect>1.1.1</me:expect>
    <iati-identifier>WXYZ-123</iati-identifier>
    <reporting-org ref="XYZ"/>
  </iati-activity>

  <!-- 1.1.2 - Duplicate identifier -->
  <me:testset>
    <me:expect>1.1.2</me:expect>

    <iati-activity>
      <iati-identifier>XYZ</iati-identifier>
    </iati-activity>

    <iati-activity>
      <iati-identifier>XYZ</iati-identifier>
    </iati-activity>
  </me:testset>

  <!-- 1.1.3 - Reporting org and activity id the same -->
  <iati-activity>
    <me:expect>1.1.3</me:expect>
    <iati-identifier>ABCXYZ</iati-identifier>
    <reporting-org ref="ABCXYZ"/>
  </iati-activity>

</iati-activities>
